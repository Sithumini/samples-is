    <soapenv:Header/>
    <soapenv:Body>
        <ser:addUser>
            <!--Optional:-->
            <ser:userName>cameron@wso2.com</ser:userName>
            <!--Optional:-->
            <ser:credential>cameron!23</ser:credential>
            <!--Zero or more repetitions:-->
